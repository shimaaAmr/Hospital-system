@model Hospital_System.Models.MedicalSpecialty

@{
	ViewData["Title"] = "Create";
}

<h1 class="alert alert-primary text-center">Create Medical Specialty</h1>


@* <h4>MedicalSpecialty</h4> *@
<hr />
<div class="row">
	<div class="col-md-3">
		<form asp-action="Create" enctype="multipart/form-data">
			<div asp-validation-summary="All" class="text-danger"></div>

			<div class="form-group">
				<label asp-for="Name" class="control-label"></label>
				<input asp-for="Name" class="form-control" />
				<span asp-validation-for="Name" class="text-danger"></span>
			</div>
			<div class="form-group row">
				<button type="submit" class=" btn btn-primary m-2  col-4">Add</button>
				<a asp-action="Index" class="btn btn-secondary m-2  col-4">Back to List</a>

			</div>
		</form>
	</div>
</div>

<div>
	@* <a asp-action="Index" class="btn btn-secondry">Back to List</a> *@
</div>

@section Scripts {
	@{
		await Html.RenderPartialAsync("_ValidationScriptsPartial");
	}
}
