@model Hospital_System.Models.MedicalSpecialty

@{
	ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>MedicalSpecialty</h4>
<hr />
<div class="row">
	<div class="col-md-4">
		<form asp-action="Edit" enctype="multipart/form-data">
			<div asp-validation-summary="All" class="text-danger"></div>
			<input type="hidden" asp-for="Id" />
			<div class="form-group">
				<label asp-for="Name" class="control-label"></label>
				<input asp-for="Name" class="form-control" />
				<span asp-validation-for="Name" class="text-danger"></span>
			</div>
			<div class="form-group " class="col-md-3">
				@* <input type="submit" value="Save" class="btn btn-primary" /> *@
				<input type="submit" value="Save" class="btn btn-success m-2  col-4" />
				<a asp-action="Index" class="btn btn-secondary m-2  col-4">Back to List</a>
			</div>
		</form>
	</div>
</div>



@section Scripts {
	@{
		await Html.RenderPartialAsync("_ValidationScriptsPartial");
	}
}
